@model Form
<form method="post">
	<div class="mb-3">
		<label asp-for="Url">Url</label>
		<input asp-for="Url" autofocus="autofocus" class="form-control" placeholder="https://example.org" />
	</div>
	<div class="mb-3 text-end">
		<button class="btn btn-primary" type="submit">Submit</button>
	</div>
</form>